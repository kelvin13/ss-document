extension SVG.Element.Attribute 
{
    /// Returns an instance of the SVG `\(__name__)` attribute, with no associated value.
    @inlinable public static 
    var __attribute__:Self 
    {
        self.init(.init(escaped: __name__))
    }
    /// Returns an instance of the SVG `\(__name__)` attribute, with the given string value.
    @inlinable public static 
    func __attribute__<S>(_ string:S) -> Self where S:StringProtocol 
    {
        self.init(.init(key: __name__, escaped: string))
    }
}